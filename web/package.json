{
    "name": "swipeforfuture",
    "version": "2.0.0",
    "scripts": {
        "dev": "svelte-kit dev",
        "build": "svelte-kit build && node scripts/postbuild.js",
        "preview": "svelte-kit preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
        "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
        "deploy": "npm run build && gh-pages -d build --dotfiles"
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "^1.0.0-next.13",
        "@sveltejs/kit": "1.0.0-next.143",
        "@types/fs-extra": "^9.0.12",
        "autoprefixer": "^10.3.1",
        "cssnano": "^5.0.7",
        "fs-extra": "^10.0.0",
        "postcss": "^8.3.6",
        "postcss-load-config": "^3.1.0",
        "prettier": "^2.3.2",
        "prettier-plugin-svelte": "^2.3.1",
        "svelte": "^3.42.1",
        "svelte-check": "^2.2.4",
        "svelte-preprocess": "^4.7.4",
        "tailwindcss": "^2.2.7",
        "tslib": "^2.3.1",
        "typescript": "^4.3.5",
        "gh-pages": "^3.2.3"
    },
    "type": "module",
    "prettier": {
        "singleQuote": true,
        "tabWidth": 4,
        "trailingComma": "all",
        "semi": false
    },
    "engines": {
        "node": ">= 16"
    },
    "engineStrict": true,
    "private": true
}
