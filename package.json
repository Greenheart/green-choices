{
    "name": "swipeforfuture",
    "version": "0.1.0",
    "repository": "https://github.com/Greenheart/swipeforfuture.com/",
    "dependencies": {
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.2",
        "@types/styled-components": "^5.1.9",
        "@types/xlsx": "0.0.36",
        "gh-pages": "^3.1.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-icons": "^4.2.0",
        "react-scripts": "^4.0.3",
        "react-spring": "^8.0.27",
        "react-with-gesture": "^4.0.8",
        "styled-components": "^5.2.1",
        "typescript": "^4.2.3",
        "xlsx": "^0.17.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
