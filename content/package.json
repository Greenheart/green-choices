        "@types/glob": "^7.1.4",
        "@types/node": "^16.4.13",
        "esbuild": "^0.12.19",
        "fs-extra": "^10.0.0",
        "gh-pages": "^3.2.3",
        "glob": "^7.1.7",
        "typescript": "^4.3.5"
